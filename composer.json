{
    "name": "tumedia/aether",
    "description": "Aether - A modular PHP framework",
    "keywords": ["aether"],
    "license": "MIT",
    "require": {
        "php": ">=5.4",
        "filp/whoops": "^2.1",
        "illuminate/config": "5.5.*",
        "illuminate/console": "5.5.*",
        "illuminate/container": "5.5.*",
        "illuminate/contracts": "5.5.*",
        "illuminate/database": "5.5.*",
        "illuminate/events": "5.5.*",
        "illuminate/support": "5.5.*",
        "psy/psysh": "^0.8",
        "sentry/sentry": "1.7.*",
        "smarty/smarty": "3.1.30",
        "symfony/var-dumper": "~3.0|~4.0",
        "vlucas/phpdotenv": "^2.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.4"
    },
    "autoload": {
        "psr-4": {
            "Aether\\": "./src"
        },
        "files": [
            "./src/class_aliases.php",
            "./src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "./tests"
        }
    },
    "bin": [
        "./bin/updateLocales",
        "./bin/aether"
    ],
    "config": {
        "sort-packages": true
    }
}
